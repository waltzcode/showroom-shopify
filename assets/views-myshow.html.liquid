<div id="homepage-products">

  <!--My code-->

  <h2 ng-if="loggedIn && myShowVideos && (myShowVideos.length > 0)"><span>My Show</span></h2>

  <ul ng-if="loggedIn && myShowVideos && (myShowVideos.length > 0)" class="product-loop"  style="width: 100%">
    <li ng-repeat="video in myShowVideos" class="show desktop-4 tablet-half mobile-full">
      <div class="show-wrapper" ng-include="'{{ 'inc_list_show.html' | asset_url }}'"></div>
    </li>
  </ul>
	
	<button class="load-more" ng-click="loadMore()" ng-show="videos && videos.length > 0" ng-disabled="!hasMore">Load More</button>
</div>
<div class="clear"></div>
