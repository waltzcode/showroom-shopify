   {{ 'angular.min.js' | asset_url | script_tag }}
   {{ 'angular-route.min.js' | asset_url | script_tag }}
   {{ 'angular-cookies.min.js' | asset_url | script_tag }}
  	{{ 'angular-sanitize.min.js' | asset_url | script_tag }}
  	{{ 'videogular.min.js' | asset_url | script_tag }}
  	{{ 'vg-controls.min.js' | asset_url | script_tag }}
  	{{ 'vg-buffering.min.js' | asset_url | script_tag }}
  	{{ 'vg-poster.min.js' | asset_url | script_tag }}
  	{{ 'vg-overlay-play.min.js' | asset_url | script_tag }}
   {{ 'srApp.js' | asset_url | script_tag }}
   {{ 'srConstants.js' | asset_url | script_tag }}
   {{ 'srServices.js' | asset_url | script_tag }}
   {{ 'srControllers.js' | asset_url | script_tag }}
   {{ 'srDirectives.js' | asset_url | script_tag }}
   {{ 'srFilters.js' | asset_url | script_tag }}
</body>
</html>
